<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="sg.edu.rpc346.id19016011.mydatabook">

    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyDataBook">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".BioFragment"
            android:parentActivityName=".MainActivity" >
        </activity>
        <activity android:name=".VaccinationFragment"
            android:parentActivityName=".MainActivity" >
        </activity>
        <activity android:name=".AnniversaryFragment"
            android:parentActivityName=".MainActivity" >
        </activity>
        <activity android:name=".AboutUsFragment"
            android:parentActivityName=".MainActivity" >
        </activity>

    </application>

</manifest>